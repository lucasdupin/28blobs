<teBlobs>
	<osc>
		<ip>127.0.0.1</ip><port>3333</port>
	</osc>
	<canvas>
		<width>1000</width>
		<height>600</height>
		<threshold>80</threshold>
		<blur>-1</blur>
		<minBlobSize>20</minBlobSize>
		<maxBlobSize>1000</maxBlobSize>
		<maxBlobs>10</maxBlobs>
	</canvas>
	<cameras>
		<camera>
			<width>320</width>
			<height>240</height>
		</camera>
	</cameras>
	
	<!-- Theese are application settings, you won't need to change it! -->
	<application>
		<textColor>
			<!-- <r>100</r><g>0</g><b>130</b> -->
			<r>200</r><g>150</g><b>0</b>
		</textColor>
		<canvasColor>
			<r>80</r><g>80</g><b>80</b>
		</canvasColor>
		<camera>
			<borderColor>
				<r>150</r><g>80</g><b>0</b>
			</borderColor>
		</camera>
	</application>
</teBlobs>